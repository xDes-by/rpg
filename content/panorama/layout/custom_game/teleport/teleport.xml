<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
        <include src="file://{resources}/layout/custom_game/teleport/teleport.css" /> 
	</styles>

	<scripts>
		<include src="file://{resources}/layout/custom_game/teleport/teleport.js" />
	</scripts>
	
	<snippets>
		<snippet name="zone_panel">
			<Panel>
				<Label id="zone_name" text="#zone"/>
				<Panel id="Panelrecommended">
					<Label id="zone_recommended" text="#zone_recommended"/>
					<Label id="zone_recommended_count" text="1"/>
				</Panel>
				<Panel id="PanelCost">
					<Panel id="PanelCostImage"/>
					<Label id="zone_tp_cost" text="#cost"/>
				</Panel>
			</Panel>
		</snippet>
	</snippets>

	<Panel style="width:100%;height:100%;" hittest="false">
		<Panel id="TeleportPanel">
			<Label id="TeleportPanelLabel" text="#teleport"/>
			<Panel id="CloseButton" onmouseactivate="Open()"/>	
			<Panel id="TeleportPanelContent"/>
		</Panel>
    </Panel>
</root>